{
  "buildCommand": "cd ../.. && corepack enable pnpm && pnpm install && pnpm build",
  "outputDirectory": "apps/web/.next",
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_ENGINE_URL": "@next_public_engine_url",
    "DATABASE_URL": "@database_url"
  },
  "regions": ["iad1"],
  "functions": {
    "api/**": {
      "maxDuration": 60
    }
  }
}
